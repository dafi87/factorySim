#!/bin/bash
#SBATCH --job-name=geneticfactorySim
#SBATCH --partition=paul
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=4
#SBATCH --mem-per-cpu=2GB
#SBATCH --time=24:00:00
#SBATCH --mail-type=BEGIN,END



apptainer exec "$HOME/factorySim/factorySim.sif" python3 -u geneticFactorySim.py --num-workers "$SLURM_CPUS_PER_TASK" --num-generations 6 --num-population 99

